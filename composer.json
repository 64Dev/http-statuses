{
    "name": "laravel-lang/http-statuses",
    "description": "List of 77 languages for HTTP statuses",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "languages", "lang", "translations", "messages", "http", "status"],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        },
        {
            "name": "Laravel-Lang Team",
            "homepage": "https://github.com/Laravel-Lang"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "laravel-lang/publisher": "^11.1",
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^9.4",
        "symfony/var-dumper": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelLang\\HttpStatuses\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "LaravelLang\\HttpStatuses\\ServiceProvider"
            ]
        }
    }
}
